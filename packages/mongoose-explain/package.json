{
  "name": "@ghostjs/mongoose-explain",
  "version": "1.0.0",
  "description": "A mongoose plugin for logging .explain() output of queries",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "contributors": [
    {
      "name": "Johannes Loher",
      "email": "johannes.loher@fg4f.com"
    }
  ],
  "keywords": [
    "mongoose",
    "mongodb",
    "query",
    "explain",
    "plugin"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "format": "prettier --write \"**/*.(js|ts|json|yml)\"",
    "lint:fix": "eslint --ignore-path ../../.eslintignore --ext .ts,.js --fix .",
    "lint": "eslint --ignore-path ../../.eslintignore --ext .ts,.js .",
    "preCommit": "lint-staged",
    "test": "jest"
  },
  "peerDependencies": {
    "mongodb": "^4.3.1",
    "mongoose": "^6.2.8"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mongodb": "^4.3.1",
    "mongodb-memory-server": "^8.4.1",
    "mongoose": "^6.2.8",
    "prettier": "^2.6.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --fix",
    "*.(json|yml)": "prettier --write"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
